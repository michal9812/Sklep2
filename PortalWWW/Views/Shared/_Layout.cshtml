<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SklepKomputerowy</title>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/lib/materialize/css/materialize.min.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <link href="~/css/site.css" type="text/css" rel="stylesheet" />

</head>
<body>
    <header>
        <nav>
            <div class="nav-wrapper">
                <a href="#" class="brand-logo">Logo</a>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a asp-controller="Sklep" asp-action="Index">Sklep Komputerowy</a></li>
                    <li><a href="badges.html">Components</a></li>
                    <li><a href="collapsible.html">JavaScript</a></li>
                </ul>
            </div>
        </nav>

    </header>
    <main role="main">
        <div class="container">
            @RenderBody()
        </div>
    </main>

    <footer class="footer-copyright">
        <div class=" green darken-4 white-text">
            1211323
        </div>

        <script src="~/lib/jquery/dist/jquery.min.js"></script>

        <script src="~/lib/materialize/js/materialize.min.js"></script>

        <script src="~/lib/materialize/js/init.js"></script>

        <script src="~/js/site.js" asp-append-version="true"></script>
        @RenderSection("Scripts", required: false)
    </footer>



</body>
</html>
